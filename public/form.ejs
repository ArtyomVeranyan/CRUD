<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <ul>
        <% info.forEach(function(one) { %>
        <li>
            Id - <strong><%= one.theaterId %></strong> <br>
            City - <strong><%= one.location.address.city %></strong> /
            State - <strong><%= one.location.address.state %></strong> /
            Zip code - <strong><%= one.location.address.zipcode %></strong> /
            Street - <strong><%= one.location.address.street1 %></strong> <br> <br>
        </li>
        <% }); %>
        </ul>
    <!-- <form action="/addName" method="post"> -->
        <input id="usName" type="text" required name="name" placeholder="Please type your name"><br><br>
        <input id="usPassword" type="password" name="age" placeholder="Please type your Pass"><br><br>
        <input id="usEmail" type="email" name="email" placeholder="Please type your email"><br><br>
        <input type="submit" onclick="getVal()" >
    <!-- </form> -->

    <script src="script.js"></script>
</body>
</html>
